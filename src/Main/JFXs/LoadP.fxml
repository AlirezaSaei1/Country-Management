<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.Controller">
    <children>
        <Button layoutX="224.0" layoutY="125.0" mnemonicParsing="false" onAction="#newProgress" prefHeight="25.0" prefWidth="153.0" style="-fx-background-color: skyBlue; -fx-z-index: 10;" text="New Progress" />
        <Button layoutX="224.0" layoutY="188.0" mnemonicParsing="false" onAction="#loadProgress" prefHeight="25.0" prefWidth="153.0" style="-fx-background-color: orange;" text="Load Progress" />
        <Button layoutX="224.0" layoutY="316.0" mnemonicParsing="false" onAction="#exit" prefHeight="25.0" prefWidth="153.0" style="-fx-background-color: red;" text="Exit" />
        <Label layoutX="174.0" layoutY="64.0" prefHeight="17.0" prefWidth="253.0" style="-fx-alignment: CENTER;" text="Progress" textFill="WHITE">
            <font>
                <Font size="37.0" />
            </font>
        </Label>
      <Label layoutX="90.0" layoutY="264.0" text="Load Progress : " textFill="WHITE" />
      <TextField fx:id="loadTextField" layoutX="200.0" layoutY="260.0" prefHeight="25.0" prefWidth="200.0" />
      <Button layoutX="427.0" layoutY="260.0" mnemonicParsing="false" onAction="#submitE" text="submit" />
      <Label fx:id="loadLabel" alignment="CENTER" layoutY="293.0" prefHeight="17.0" prefWidth="600.0" textFill="#f50606" />
    </children>
</AnchorPane>
